<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>css-3d旋转</title>
<link rel="stylesheet" href="css/rotate3d.css" />
</head>
<body>
		<!--/*外层最大容器*/-->
		<div class="wrap">
	<!--	/*包裹所有元素的容器*/-->
		<div class="cube">
			<!--前面图片 -->
			<div class="out_front">
				<img src="img/rotate3d/1.jpg"  class="pic"/>
			</div>
			<!--后面图片 -->
			<div class="out_back">
				<img src="img/rotate3d/2.jpg"  class="pic"/>
			</div>
			<!--左图片 -->
			<div class="out_left">
				<img src="img/rotate3d/3.jpg"  class="pic"/>
			</div>
			<div class="out_right">
				<img src="img/rotate3d/4.jpg"  class="pic"/>
			</div>
			<div class="out_top">
				<img src="img/rotate3d/5.jpg"  class="pic"/>
			</div>
			<div class="out_bottom">
				<img src="img/rotate3d/6.jpg"  class="pic"/>
			</div>
			<!--小正方体 --> 
			<span class="in_front">
				<img src="img/rotate3d/7.jpg" class="in_pic" />
			</span>
			<span class="in_back">
			     <img src="img/rotate3d/8.jpg" class="in_pic" />
			</span>
			<span class="in_left">
				<img src="img/rotate3d/9.jpg" class="in_pic" />
			</span>
			<span class="in_right">
				<img src="img/rotate3d/10.jpg" class="in_pic" />
			</span>
			<span class="in_top">
				<img src="img/rotate3d/11.jpg" class="in_pic" />
			</span>
			<span class="in_bottom">
				<img src="img/rotate3d/12.jpg" class="in_pic" />
			</span>
		</div>
		</div>
	</body>
</html>